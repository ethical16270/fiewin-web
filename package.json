{
  "name": "fiewin-web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.16.14",
    "@mui/x-charts": "^7.27.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "framer-motion": "^12.4.5",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "node-fetch": "^2.6.9",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.2.0",
    "react-scripts": "5.0.1",
    "sqlite3": "^5.1.7",
    "ws": "^8.16.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:prod": "NODE_ENV=production pm2 start ecosystem.config.js --env production",
    "build": "PUBLIC_URL='' react-scripts build",
    "server": "PORT=4000 nodemon server.js",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "init-db": "node server/utils/initDb.js",
    "test": "react-scripts test",
    "test:prod": "cross-env NODE_ENV=production jest --coverage --passWithNoTests",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "nodemon": "^3.1.9"
  },
  "proxy": "http://localhost:4000",
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@babel|@mui)/)"
    ]
  }
}
